{
	"name": "@snickbit/out",
	"version": "2.1.4",
	"description": "Cross-platform pretty output for your terminal or browser console.",
	"license": "MIT",
	"exports": {
		"require": "./dist/index.js",
		"import": "./dist/index.mjs",
		"types": "./dist/index.d.ts"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup --minify",
		"check": "pnpm run lint && pnpm run typecheck",
		"dev": "tsup --watch",
		"lint": "eslint \"**/*.{ts,json,yml,yaml}\" --cache --color",
		"lint:fix": "pnpm run lint --fix",
		"test": "jest --ci",
		"typecheck": "tsc --noEmit",
		"docs": "typedoc",
		"start": "node ./examples/node.js"
	},
	"release": {
		"extends": [
			"@snickbit/semantic-release"
		]
	},
	"jest": {
		"preset": "ts-jest",
		"testEnvironment": "node"
	},
	"dependencies": {
		"@snickbit/ansi": "0.0.26",
		"@snickbit/cycle": "1.0.22",
		"@snickbit/utilities": "3.11.1",
		"ansi-styles-template": "1.0.0",
		"browser-or-node": "2.1.1",
		"node-inspect-extracted": "3.0.0",
		"picomatch-browser": "2.2.6"
	},
	"devDependencies": {
		"@snickbit/eslint-config": "1.5.3",
		"@snickbit/semantic-release": "1.6.6",
		"@types/jest": "29.5.3",
		"@types/node": "18.16.2",
		"concurrently": "8.2.0",
		"eslint": "8.45.0",
		"jest": "29.6.1",
		"semantic-release": "20.1.3",
		"ts-jest": "29.1.1",
		"tsup": "7.1.0",
		"typedoc": "0.24.8",
		"typedoc-plugin-markdown": "3.15.3",
		"typescript": "5.1.6"
	},
	"engines": {
		"node": ">= 16"
	},
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"dts": true,
		"clean": true,
		"format": [
			"esm",
			"cjs"
		]
	}
}
